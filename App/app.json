{
  "expo": {
    "name": "Bull Sniffer",
    "slug": "BareBodkin",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#232b2b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bullsniffer",
      "requireFullScreen": true,
      "buildNumber": "4"
    },
    "android": {
      "package": "com.bullsniffer",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Bull Sniffer to access your camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "17399653-9ede-46a3-9d36-975b5ff7341d"
      }
    }
  }
}
